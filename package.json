{
  "name": "weather-electron-app",
  "version": "1.0.0",
  "description": "Electron app with Svelte that shows current weather",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "keywords": [
    "electron",
    "svelte",
    "weather"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "concurrently": "^8.2.2",
    "electron": "^39.2.5",
    "electron-builder": "^24.9.1",
    "svelte": "^4.2.8",
    "vite": "^7.2.6",
    "wait-on": "^7.2.0"
  }
}
